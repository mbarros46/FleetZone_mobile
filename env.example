# FleetZone Mobile - Configuração de Ambiente

# URL da API Backend
EXPO_PUBLIC_API_URL=http://localhost:8080

# Configurações do App
EXPO_PUBLIC_APP_NAME=FleetZone
EXPO_PUBLIC_APP_VERSION=1.0.0

# Configurações de Desenvolvimento
EXPO_PUBLIC_DEBUG=true
EXPO_PUBLIC_LOG_LEVEL=info

# Configurações de Notificações (opcional)
EXPO_PUBLIC_FCM_SERVER_KEY=your_fcm_server_key_here

# Configurações de Analytics (opcional)
EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here

